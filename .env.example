# Frontend Environment Variables Example
# Rename this file to .env.local to use

# API URL for backend classification service
VITE_API_URL=http://localhost:8000

# Enable debug logging
VITE_DEBUG=false

# Dashboard refresh interval in milliseconds (default: 30000 = 30 seconds)
VITE_DASHBOARD_REFRESH_INTERVAL=30000

# Maximum number of calls to display in history
VITE_MAX_HISTORY_ITEMS=50
